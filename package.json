{
  "name": "@oceanprotocol/pleuston",
  "version": "0.3.1",
  "license": "Apache-2.0",
  "scripts": {
    "start": "node --max_old_space_size=4096 scripts/start.js",
    "build": "node --max_old_space_size=4096 scripts/build.js",
    "lint:js": "eslint ./src/",
    "lint:css": "stylelint ./src/**/*.scss",
    "lint": "run-p --continue-on-error lint:js lint:css",
    "test": "npm run lint && node scripts/test.js --env=jsdom --coverage --watchAll=false",
    "coverage": "cat ./coverage/lcov.info | codacy-coverage",
    "release": "release-it --non-interactive",
    "changelog": "auto-changelog -p"
  },
  "engines": {
    "node": ">=8 <12"
  },
  "dependencies": {
    "@oceanprotocol/art": "^2.2.0",
    "@oceanprotocol/squid": "^0.5.14",
    "aws-amplify": "^1.1.28",
    "aws-amplify-react": "^2.3.8",
    "azure-storage": "^2.10.3",
    "connected-react-router": "^6.4.0",
    "copy": "^0.3.2",
    "core-js": "^3.1.3",
    "elliptic": "^6.4.1",
    "fetch-download": "^1.1.1",
    "fs-extra": "^8.0.1",
    "history": "^4.9.0",
    "is-url": "^1.2.4",
    "jsonwebtoken": "^8.5.1",
    "object-assign": "^4.1.1",
    "promise": "^8.0.3",
    "prop-types": "^15.7.2",
    "query-string": "^6.5.0",
    "react": "^16.8.6",
    "react-blockies": "^1.4.1",
    "react-dom": "^16.8.6",
    "react-modal": "^3.8.1",
    "react-oauth-flow": "^1.2.0",
    "react-redux": "^7.0.3",
    "react-router": "^5.0.0",
    "react-router-dom": "^5.0.0",
    "react-select": "^3.0.4",
    "react-transition-group": "^4.1.0",
    "react-truncate": "^2.4.0",
    "react-vis": "^1.11.7",
    "react-web3-provider": "^1.1.2",
    "redux": "^4.0.1",
    "redux-devtools-extension": "^2.13.8",
    "redux-form": "^8.2.3",
    "redux-thunk": "^2.3.0",
    "secp256k1": "^3.7.1",
    "web3": "1.0.0-beta.37"
  },
  "devDependencies": {
    "@babel/core": "^7.4.5",
    "@babel/polyfill": "^7.4.4",
    "@babel/preset-env": "^7.4.5",
    "@babel/preset-react": "^7.0.0",
    "@svgr/webpack": "^4.3.0",
    "auto-changelog": "^1.13.0",
    "babel-core": "^7.0.0-bridge.0",
    "babel-eslint": "^10.0.1",
    "babel-jest": "^24.8.0",
    "babel-loader": "^8.0.6",
    "babel-plugin-named-asset-import": "^0.3.2",
    "babel-preset-react-app": "^9.0.0",
    "case-sensitive-paths-webpack-plugin": "^2.2.0",
    "chalk": "^2.4.2",
    "codacy-coverage": "^3.4.0",
    "css-loader": "^2.1.1",
    "dotenv": "^8.0.0",
    "dotenv-expand": "^5.1.0",
    "eslint": "^5.16.0",
    "eslint-config-oceanprotocol": "^1.3.0",
    "eslint-config-standard": "^12.0.0",
    "eslint-config-standard-react": "^7.0.2",
    "eslint-loader": "^2.1.2",
    "eslint-plugin-node": "^9.1.0",
    "file-loader": "^3.0.1",
    "html-webpack-plugin": "^4.0.0-beta.4",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^24.8.0",
    "mini-css-extract-plugin": "^0.7.0",
    "node-sass": "^4.12.0",
    "npm-run-all": "^4.1.5",
    "optimize-css-assets-webpack-plugin": "^5.0.1",
    "pnp-webpack-plugin": "^1.4.3",
    "postcss-flexbugs-fixes": "^4.1.0",
    "postcss-loader": "^3.0.0",
    "postcss-normalize": "^7.0.1",
    "postcss-preset-env": "^6.6.0",
    "postcss-safe-parser": "^4.0.1",
    "react-dev-utils": "^9.0.1",
    "release-it": "^12.2.2",
    "sass-loader": "^7.1.0",
    "style-loader": "^0.23.1",
    "stylelint": "^10.0.1",
    "stylelint-config-bigchaindb": "^1.2.2",
    "stylelint-config-css-modules": "^1.4.0",
    "stylelint-config-standard": "^18.3.0",
    "terser-webpack-plugin": "^1.3.0",
    "thread-loader": "^2.1.1",
    "url-loader": "^1.1.2",
    "webpack": "^4.32.2",
    "webpack-dev-server": "^3.5.1",
    "webpack-manifest-plugin": "^2.0.4",
    "workbox-webpack-plugin": "^4.3.1"
  },
  "browserslist": {
    "development": [
      "last 2 versions"
    ],
    "production": [
      ">1%",
      "last 4 versions",
      "Firefox ESR",
      "not ie <= 11"
    ]
  },
  "jest": {
    "collectCoverageFrom": [
      "src/**/*.{js,jsx,mjs}"
    ],
    "setupFiles": [],
    "testMatch": [
      "<rootDir>/src/**/__tests__/**/*.{js,jsx,mjs}",
      "<rootDir>/src/**/?(*.)(spec|test).{js,jsx,mjs}"
    ],
    "testEnvironment": "node",
    "testURL": "http://localhost",
    "transform": {
      "^.+\\.(js|jsx|mjs)$": "<rootDir>/config/jest/babelTransform.js",
      "^.+\\.css$": "<rootDir>/config/jest/cssTransform.js",
      "^(?!.*\\.(js|jsx|mjs|css|json|graphql)$)": "<rootDir>/config/jest/fileTransform.js"
    },
    "transformIgnorePatterns": [
      "[/\\\\]node_modules[/\\\\].+\\.(js|jsx|mjs)$",
      "^.+\\.module\\.(css|sass|scss)$"
    ],
    "moduleNameMapper": {
      "^react-native$": "react-native-web",
      "^.+\\.module\\.(css|sass|scss)$": "identity-obj-proxy"
    },
    "moduleFileExtensions": [
      "web.js",
      "js",
      "json",
      "web.jsx",
      "jsx",
      "node",
      "mjs"
    ]
  },
  "release-it": {
    "scripts": {
      "beforeStart": "npm test",
      "beforeStage": "npm run changelog",
      "afterRelease": "echo Successfully released ${name} v${version} to ${repo.repository}."
    },
    "git": {
      "tagName": "v${version}"
    },
    "github": {
      "release": true
    },
    "npm": {
      "publish": false
    }
  }
}
